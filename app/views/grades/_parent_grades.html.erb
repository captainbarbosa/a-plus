<table class="table-index table">
  <thead>
    <tr>
      <th>Assignment</th>
      <th>Score</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @parent_grades.each do |grade| %>
      <tr>
        <td><%= grade.assignment_name %></td>
        <td><%= grade.grade %></td>
        <td><%= grade.updated_at.strftime("%m/%d/%Y")%></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
