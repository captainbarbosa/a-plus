<p id="notice"><%= notice %></p>

<h1>Listing Students</h1>

<button type="button" class="btn btn-default"><%= link_to 'New Student', new_student_path %></button>
<button type="button" class="btn btn-default"><%= link_to 'Back to Dashboard', root_path %></button>


<table class="table-index table">
   <thead>
      <tr>
         <th>All Students</th>
      </tr>
   </thead>
   <tbody>
     <% @students.each do |student| %>
      <tr>
          <td class="column-1"><%= link_to student.name, student %></td>
      </tr>
    <% end %>
   </tbody>
</table>
